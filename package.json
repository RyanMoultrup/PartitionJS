{
  "name": "@rmoultrup/partitionjs",
  "version": "0.0.8",
  "Description": "Array utility for parsing JavaScript arrays into partitions",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/RyanMoultrup/PartitionJS.git"
  },
  "author": {
    "name": "Ryan Moultrup",
    "url": "https://github.com/RyanMoultrup"
  },
  "keywords": [
    "array utility",
    "partition",
    "node",
    "browser",
    "array partition",
    "async array",
    "javascript"
  ],
  "files": [
    "dist/**/*.js",
    "src/**/*.js"
  ],
  "exports": {
    ".": {
      "require": "./dist/partitionjs.node.cjs",
      "import": "./src/index.js"
    }
  },
  "module": "./src/index.js",
  "main": "dist/partitionjs.node.cjs",
  "browser": "dist/partitionjs.umd.min.js",
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-url": "^8.0.1",
    "babel-jest": "^29.6.1",
    "cross-env": "^7.0.3",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "rollup": "^3.26.2"
  },
  "scripts": {
    "build": "rollup -c",
    "test": "jest --config=jest.config.cjs"
  }
}
